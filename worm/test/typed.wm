@worm
def pow(a: float, n: int) -> float:
    result: float = 1.0
    partial: float = a
    while n > 0:
        if n % 2 == 1:
            result = result * partial
        partial = partial * partial
        n = n // 2
    return result


@worm.entry
def main():
    printf("%f\n", pow(1.5, 4))  # print 5.062500
