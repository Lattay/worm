"""#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>
int fib(int,int,int);
void main(){
printf("%d\\n", fib(0, 1, 8));
printf("%d\\n", fib(0, 1, 10));
}
int fib(int a, int b, int n) {
while(n>0){
int c = a
a = a + b
b = c
n -= 1
}
return a;
}"""

from worm import new_prog, worm

prog = new_prog()

@worm(prog)
def fib(a: int, b: int, n: int = 1) -> int:
    while n > 0:
        c = a
        a = a + b
        b = c
        n = n - 1
    return a

@worm.entry(prog)
def main():
    printf("%d\n", fib(0, 1, 8))
    printf("%d\n", fib(0, 1, 10))

