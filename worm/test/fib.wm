from worm import new_prog

prog = new_prog()

@prog
def fib(a: int, b: int, n: int = 1) -> int:
    while n > 0:
        c = a
        a = a + b
        b = c
        n = n - 1
    return a

@prog.entry
def main():
    println("Fibonacci terms 8 and 10")
    print_int(fib(0, 1, 8))
    print_int(fib(0, 1, 10))

