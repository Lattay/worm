with worm.scope(point=Struct(x=float, y=float)):
    @worm
    def printp(p: point) -> void:
        printf("(%f, %f)\n", p.x, p.y)


    @worm.entry
    def f():
        p1: point = {x: 1.4, y: 4.5}
        printp(p1)
