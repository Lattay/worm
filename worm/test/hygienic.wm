from worm import context

worm = context.new_prog()

@worm.block
def swap(a, b):
    c = a
    a = b
    b = c

@worm.entry
def f():
    a: int = 1
    b: int = 3
    c: int = 4
    swap(a=b, b=c)
    printf("%d %d %d\n", a, b, c)  # print 1 4 3
